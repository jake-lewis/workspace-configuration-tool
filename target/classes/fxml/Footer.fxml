      </ToolBar>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" HBox.hgrow="SOMETIMES">
        <items>
            <Label text="Version 0.5" />
        </items>
      </ToolBar>
   </children>
</HBox>
